[32m+[m[32m    async function loadProfile() {[m
[32m+[m[32m      if (!userId) return;[m
[32m+[m[32m      try {[m
[32m+[m[32m        const snap = await getDoc(doc(db, `users/${userId}/profile`));[m
[32m+[m[32m        setProfile(snap.exists() ? snap.data() : { xpPoints: 0, rank: 'Recruit', rankLevel: 1 });[m
[32m+[m[32m      } catch (e) {[m
[32m+[m[32m        setProfile({ xpPoints: 0, rank: 'Recruit', rankLevel: 1 });[m
[32m+[m[32m      } finally {[m
[32m+[m[32m        setLoading(false);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    loadProfile();[m
[32m+[m[32m  }, [userId]);[m
